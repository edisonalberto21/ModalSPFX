{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "modal-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/modal/ModalWebPart.js",
          "manifest": "./src/webparts/modal/ModalWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery":{
      "path": "node_modules/jquery/dist/jquery.min.js",
      "globalName": "jQuery"
    }
  },
  "localizedResources": {
    "ModalWebPartStrings": "lib/webparts/modal/loc/{locale}.js"
  }
}
